<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.kodg.kodgProject1.join.JoinMapper">

    <insert id="save" parameterType="com.kodg.kodgProject1.join.JoinDto.SaveDto">
        INSERT INTO users
        (user_id
        , user_pw
        , user_name
        , permission
        ) VALUES (
            #{userId}
            ,#{userPw}
            ,#{userName}
            ,#{permission}
        )
    </insert>

</mapper>
